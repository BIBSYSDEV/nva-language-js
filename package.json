{
  "name": "nva-language",
  "version": "1.0.0",
  "description": "",
  "main": "LanguageMapper",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext .mjs --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "mocha": "mocha --full-trace \"src/tests/**/*.js\"",
    "test": "npm run lint && npm run mocha",
    "coverage": "c8 --all npm run test && c8 check-coverage --lines 100 --functions 100 --branches 100 && c8 report",
    "build": "npm run test && babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BIBSYSDEV/nva-language-js.git"
  },
  "keywords": [
    "NVA language mapper"
  ],
  "author": "Rurik Thomas Greenall",
  "license": "MIT",
  "volta": {
    "node": "14.18.0",
    "npm": "7.24.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "c8": "^7.9.0",
    "chai": "^4.3.4",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^9.1.2"
  }
}
